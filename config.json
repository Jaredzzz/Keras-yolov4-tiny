	"test": {
        "test_image_folder":   "/data/zjj_workspace/RDNet_data/dataset/images/test/",
        "test_annot_folder":   "/data/zjj_workspace/RDNet_data/dataset/annotations/test/",
        "cache_name":           "data/RDNet_test.pkl",
        "iou_threshold":        0.5,
